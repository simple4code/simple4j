<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.simple4code.simple4j.demo.system.mapper.PermissionPointMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.simple4code.simple4j.demo.system.entity.PermissionPoint">
        <id column="id" property="id" />
        <result column="point_class" property="pointClass" />
        <result column="point_icon" property="pointIcon" />
        <result column="point_status" property="pointStatus" />
