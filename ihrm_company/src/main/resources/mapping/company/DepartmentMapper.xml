<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.simple4code.simple4j.demo.company.mapper.DepartmentMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.simple4code.simple4j.demo.company.entity.Department">
        <id column="id" property="id" />
        <result column="company_id" property="companyId" />
        <result column="pid" property="pid" />
        <result column="name" property="name" />
        <result column="code" property="code" />
        <result column="manager" property="manager" />
        <result column="introduce" property="introduce" />
        <result column="create_time" property="createTime" />
        <result column="manager_id" property="managerId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, company_id, pid, name, code, manager, introduce, create_time, manager_id
    </sql>

</mapper>
